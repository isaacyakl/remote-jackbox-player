{
	"name": "remote-jackbox-player",
	"version": "1.0.1",
	"browser": "dist/script.js",
	"scripts": {
		"dev": "npm run build:js -- --watch & npm run build:css -- --watch",
		"build": "npm run build:js && npm run build:css && cp \"src/index.html\" \"dist/index.html\"",
		"build:js": "babel src -d dist",
		"build:css": "npx tailwindcss -i ./src/input.css -o ./dist/output.css"
	},
	"author": "Isaac 'yak' L.",
	"license": "MIT",
	"description": "Play Jackbox games with your friends remotely.",
	"devDependencies": {
		"@babel/cli": "^7.20.7",
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"tailwindcss": "^3.1.6"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/isaacyakl/remote-jackbox-player.git"
	},
	"keywords": [
		"jackbox",
		"remote",
		"player"
	],
	"bugs": {
		"url": "https://github.com/isaacyakl/remote-jackbox-player/issues"
	},
	"homepage": "https://github.com/isaacyakl/remote-jackbox-player#readme"
}
